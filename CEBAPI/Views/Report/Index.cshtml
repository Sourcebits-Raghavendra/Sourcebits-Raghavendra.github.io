@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>CEB MoCog - Report</title>
    <style>
        body {
             background:url('/CEBAPI/Images/ceb-background.jpg') no-repeat center center fixed;
             background-size: cover;
             -webkit-background-size: cover;
             -moz-background-size: cover;
             -o-background-size: cover;
             background-size: cover;
        }

        @@font-face {
    	    font-family: 'proxima_regular';
    	    src: url('/CEBAPI/fonts/proximanova-reg.eot');
    	    src: url('/CEBAPI/fonts/proximanova-reg.eot?#iefix') format('embedded-opentype'), url('/CEBAPI/fonts/proximanova-reg.woff2') format('woff2'), url('/CEBAPI/fonts/proximanova-reg.woff') format('woff'), url('/CEBAPI/fonts/proximanova-reg.ttf')format('truetype'), url('/CEBAPI/fonts/proximanova-reg.svg#proximaregular') format('svg');
    	    font-weight: 100;
    	    font-style: normal;
	    }

        @@font-face {
    	    font-family: 'proxima_semibold';
    	    src: url('/CEBAPI/fonts/proximanova-semibold.eot');
   	        src: url('/CEBAPI/fonts/proximanova-semibold.eot?#iefix') format('embedded-opentype'), url('/CEBAPI/fonts/proximanova-semibold.woff2') format('woff2'), url('/CEBAPI/fonts/proximanova-semibold.woff') format('woff'), url('/CEBAPI/fonts/proximanova-semibold.ttf') format('truetype'), url('/CEBAPI/fonts/proximanova-semibold.svg#proxima_semibold') format('svg');
    	    font-weight: normal;
    	    font-style: normal;
	    }

	    @@font-face {
   	        font-family: 'proxima_bold';
   	        src: url('/CEBAPI/fonts/proximanova-bold.eot');
    	    src: url('/CEBAPI/fonts/proximanova-bold.eot?#iefix') format('embedded-opentype'), url('/CEBAPI/fonts/proximanova-bold.woff2') format('woff2'), url('/CEBAPI/fonts/proximanova-bold.woff') format('woff'), url('/CEBAPI/fonts/proximanova-bold.ttf') format('truetype'), url('/CEBAPI/fonts/proximanova-bold.svg#proxima_rgbold') format('svg');
    	    font-weight: normal;
    	    font-style: normal;
	    }

		.ceb-background{
		    background:url('/CEBAPI/Images/ceb-background.jpg') no-repeat center center fixed;
		    background-size: cover;
		}

		.width-500 {
		    max-width: 500px;
		    margin: auto;
		}

		.info-box {
		    margin: 10px auto;
		    font-size: 16px;
		}

		.info-bar{
		    height: 50px;
		    border-radius: 5px 5px 0 0;
		    /*box-shadow: 0 1px 0 0 rgba(0, 0, 0, 0.16), inset 0 1px 0 0 rgba(255, 255, 255, 0.9);*/
		    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
		     background-image: linear-gradient(to bottom, #f9f9f9, #e5e5e5);
		    /*background: linear-gradient(#f7f
		    7f7, #ebebeb);*/
		    border-bottom: 1px solid #ccc;
		}

		.info-bar .info-title{
		    text-transform: uppercase;
		    color: #0e406a;
		    font-size: 18px;
		    font-family: 'proxima_semibold';
            margin-top: 222px;
		    padding: 14px;
		}

		.info-box .info-box-content-wrap{
		    padding:15px;
		    border-radius: 0 0 5px 5px;
		    background-color: rgba(255, 255, 255, 0.9);
		}

		.info-box-content-wrap p {
		    margin: 0 0 2px;
		    font-family: 'proxima_regular';
		}

		.btn-ceb {
		    outline: none;
		    margin-top: 15px;
		    font-size: 16px;
		    color: rgb(255, 255, 255);
		    height: 44px;
            width: 100%;
		    border-radius: 0px;
		    font-family: 'proxima_semibold';
		    border: 0px;
		    color: rgb(255, 255, 255);
		    /* For browsers that do not support gradients */
		    background: -webkit-linear-gradient(rgb( 0, 200, 243), rgb( 0, 151, 243));
		    /* For Safari 5.1 to 6.0 */
		    background: -o-linear-gradient(rgb( 0, 200, 243), rgb( 0, 151, 243));
		    /* For Opera 11.1 to 12.0 */
		    background: -moz-linear-gradient(rgb( 0, 200, 243), rgb( 0, 151, 243));
		    /* For Firefox 3.6 to 15 */
		    background: linear-gradient(rgb( 0, 200, 243), rgb( 0, 151, 243));
		    -webkit-box-shadow: 0px 6px 6px 0px rgba(0, 0, 0, 0.15), 0px 3px 8px 0px rgba(0, 199, 243, 0.30);
		    -moz-box-shadow: 0px 6px 6px 0px rgba(0, 0, 0, 0.15), 0px 3px 8px 0px rgba(0, 199, 243, 0.30);
		    box-shadow: 0px 6px 6px 0px rgba(0, 0, 0, 0.15), 0px 3px 8px 0px rgba(0, 199, 243, 0.30);
    </style>
</head>
<body>
    <div class="width-500 info-box">
        <div class="info-bar">
            <p class="info-title">Report Generation</p>
        </div>
        <div class="info-box-content-wrap">
            <p class="exit-info">Please push the button below to generate a report for the Working with Information Prototype</p>
            <div style="text-align: center;">
                <button class="btn btn-ceb" ng-click='' onclick="location.href='@Url.Action("DWINFOReport", "Report")'">DOWNLOAD DWINFO REPORT</button>
            </div>
        </div>
        <div class="info-box-content-wrap">
            <p class="exit-info">Please push the button below to generate a report for the Working with Information Prototype</p>
            <div style="text-align: center;">
                <button class="btn btn-ceb" ng-click='' onclick="location.href='@Url.Action("FIReport", "Report")'">DOWNLOAD FI REPORT</button>
            </div>
        </div>
    </div>
</body>
</html>